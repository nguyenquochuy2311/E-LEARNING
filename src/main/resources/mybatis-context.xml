		<mapper resource="mapper/notification-admin.xml"/>
		<mapper resource="mapper/notice-mapper.xml" />
		<mapper resource="mapper/reference-mapper.xml" />
		<mapper resource="mapper/lesson-mapper.xml" />
		<mapper resource="mapper/order-mapper.xml"/>
