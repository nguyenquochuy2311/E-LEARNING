		<typeAlias alias="course" type="com.coeding.spring.vo.Course"/>
		<typeAlias alias="book" type="com.coeding.spring.vo.Book"/>
		<typeAlias alias="job" type="com.coeding.spring.vo.Job"/>
		<typeAlias alias="interest" type="com.coeding.spring.vo.Interest"/>
		<typeAlias alias="candidate" type="com.coeding.spring.vo.Candidate"/>
